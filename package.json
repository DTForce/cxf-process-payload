{
    "name": "inovis-payload-process",
    "displayName": "Inovis Process Payload",
    "description": "Format CXF payloads",
    "version": "0.1.0",
    "publisher": "dtforce",
    "engines": {
        "vscode": "^1.32.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.processPayloadXML"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.processPayloadXML",
                "title": "Process Payload (XML)"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "tslint ./src/*.ts",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^6.0.40",
        "tslint": "^5.16.0",
        "typescript": "^3.5.1",
        "@types/vscode": "^1.32.0"
    }
}
